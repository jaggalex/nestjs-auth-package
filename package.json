{
  "name": "@yagas-cat/nestjs-auth",
  "version": "1.0.0",
  "description": "NestJS authentication and authorization package with JWT validation, permission and role-based access control",
  "type": "module",
  "author": "Yagas Cat",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "publish:beta": "npm publish --tag beta",
    "publish:latest": "npm publish"
  },
  "keywords": [
    "nestjs",
    "authentication",
    "authorization",
    "jwt",
    "guards",
    "permissions",
    "roles",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaggalex/nestjs-auth-package.git"
  },
  "bugs": {
    "url": "https://github.com/jaggalex/nestjs-auth-package/issues"
  },
  "homepage": "https://github.com/jaggalex/nestjs-auth-package#readme",
  "peerDependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axios": "^1.11.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "rxjs": "^7.8.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
